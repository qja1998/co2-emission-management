ole.log(url)\r\n        axios.get(url,config).then(res=>{\r\n          server_total_data.value = res.data\r\n        })\r\n        .catch(error => {\r\n          console.log(error)\r\n        })\r\n        .finally(()=>{\r\n\r\n        })\r\n      }\r\n\r\n      var standardInfo = computed(()=>store.state.infopage)\r\n\r\n      function change_company(){\r\n        get_total_data()\r\n        store.commit(\"insight_select_company\",selected_company.value)\r\n      }\r\n\r\n      return{\r\n        group_list, selected_company, standardInfo,change_company\r\n      }\r\n    }\r\n  }\r\n </script>"],"mappings":"AAyDA,OAAOA,UAAS,MAAO,6BAA4B;AACnD,OAAOC,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,gBAAe,MAAO,yDAAwD;AACrF,OAAOC,gBAAe,MAAO,yCAAwC;AACrE,OAAOC,gBAAe,MAAO,yCAAwC;AACrE,OAAOC,uBAAsB,MAAO,sDAAqD;AACzF,OAAOC,mBAAkB,MAAO,kDAAiD;AACjF,OAAOC,mBAAkB,MAAO,kDAAiD;AACjF,OAAOC,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,SAAQC,QAAQ,EAAEC,GAAG,QAAO,KAAI;AAChC,OAAOC,mBAAkB,MAAO,uDAAsD;AAGpF,eAAe;EACbC,IAAG,EAAG,YAAY;EAClBC,UAAU,EAAC;IACTd,UAAU;IACVC,iBAAiB;IACjBC,gBAAgB;IAChBC,gBAAgB;IAChBC,gBAAgB;IAChBC,uBAAuB;IACvBC,mBAAmB;IACnBC,mBAAmB;IACnBK;EACF,CAAC;EACDG,MAAM,EAAE;IACNC,QAAQA,CAACC,aAAa,EAAC;MACrB,IAAI,CAACA,aAAY,GAAIA,aAAY;MACjCC,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEF,aAAa;IACjD;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIZ,QAAQ,EAAE;IACxB,IAAIa,KAAI,GAAIZ,QAAQ,CAAC,MAAMW,KAAK,CAACE,KAAK,CAACC,aAAa,GAAC,CAAC,CAAC;IACvD,IAAIC,IAAG,GAAIf,QAAQ,CAAC,MAAMW,KAAK,CAACE,KAAK,CAACG,YAAY,CAAC;IACnD,IAAIC,MAAK,GAAIhB,GAAG,CAAC,CAAC;IAClB,IAAIiB,MAAK,GAAIjB,GAAG,CAAC,CAAC;IAClB,IAAIkB,MAAK,GAAIlB,GAAG,CAAC,CAAC;IAClB,IAAImB,cAAa,GAAInB,GAAG,CAAC,CAAC;IAC1B,IAAIoB,eAAc,GAAIpB,GAAG,CAAC,CAAC;IAC3B,IAAIqB,UAAS,GAAItB,QAAQ,CAAC,MAAMW,KAAK,CAACE,KAAK,CAACS,UAAU,CAAC,CAACC,KAAI;IAC5D,IAAIC,gBAAe,GAAIvB,GAAG,CAACqB,UAAU,CAAC,CAAC,CAAC;IACxCX,KAAK,CAACc,MAAM,CAAC,wBAAwB,EAACD,gBAAgB,CAACD,KAAK;IAE5D,MAAMG,MAAK,GAAI;MACXC,OAAO,EAAC;QACJC,aAAa,EAAC,QAAQ,GAAC,GAAG,GAACjB,KAAK,CAACE,KAAK,CAACgB,W