nt:0},\n            ]\n            var sortCategory = ref(sort(category,14))\n\n            const sortReverse=()=> {\n                sortCategory.value=sortCategory.value.reverse()\n            }\n            \n            function percent(arr,size){\n                for (var i=0; i<size; i++){\n                    arr[i].percent = ((arr[i].predictData - arr[i].data)/arr[i].data).toFixed(2)\n                }\n                return arr\n            }\n\n            function sort(arr,size){\n                arr = percent(arr,size)\n\n                var key\n                var img_key\n                var name_key\n                var data_key\n                var predict_key\n                \n                for(var i=1; i<size; i++){\n                    key=arr[i].percent\n                    data_key = arr[i].data\n                    img_key = arr[i].img\n                    predict_key = arr[i].predictData\n                    name_key = arr[i].name\n                    for(var j=i-1; j>=0; j--){\n                        if(arr[j].percent >key){\n                            arr[j+1].percent = arr[j].percent\n                            arr[j+1].data = arr[j].data\n                            arr[j+1].predictData = arr[j].predictData\n                            arr[j+1].name = arr[j].name\n                        }\n                        else{\n                            break\n                        }\n                    }   \n                    arr[j+1].percent=key\n                    arr[j+1].data = data_key\n                    arr[j+1].predictData = predict_key\n                    arr[j+1].img = img_key\n                    arr[j+1].name = name_key\n                }\n                console.log(arr)\n                return arr\n            }\n\n            function transPercent(arr){\n                for(i=0; i<arr.length(); i++){\n                    arr[i].percent = arr[i].gapData/arr[i].data\n                }\n                return arr\n 