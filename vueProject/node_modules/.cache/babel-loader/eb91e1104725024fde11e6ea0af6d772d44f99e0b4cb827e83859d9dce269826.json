"_component_LoadingBar","_hoisted_9"],"sources":["C:\\Users\\user\\Desktop\\CarbonVueproject\\co2-emission-management\\vueProject\\src\\views\\Nature\\carbonNatureProgress.vue"],"sourcesContent":["<template>\r\n    <div class=\"measure-main\">\r\n        <navigation class=\"navigation\"/>\r\n        <div class=\"contents\" style=\"height:145vh\">\r\n            <nature_header/>\r\n            <div class=\"background\">\r\n                <select class=\"select_group\" v-model=\"selected_company\" @change=\"change_company(selected_company)\">\r\n                  <option v-for=\"item in group_list\" :key=\"item\">{{ item }}</option>\r\n                </select>\r\n                <div class=\"header-page\">탄소 배출량 감축 진행상황</div>\r\n                <div v-if=\"server == 7\">\r\n                    <div class =\"dash\">\r\n                        <progress_dash1 :key=\"rerender_signal\" id=\"progress-dash1\"></progress_dash1>\r\n                    </div>\r\n                    \r\n                    <progress_dash2 :key=\"rerender_signal\" id=\"progress-dash2\"></progress_dash2>\r\n                    <progress_dash3 :key=\"rerender_signal\" id=\"progress-dash3\"></progress_dash3>\r\n                    <progress_dash4 :key=\"rerender_signal\" id=\"progress-dash4\"></progress_dash4>\r\n                </div>\r\n\r\n                <div v-else-if=\"server!=7\">\r\n                    <div class=\"progress-bar\">\r\n                        <LoadingBar></LoadingBar>\r\n                        <div class=\"loding-title\">탄소 감축 진행사항을 가져오는 중입니다.</div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    \r\n</template>\r\n\r\n<script>\r\nimport navigation from \"@/components/Navigation.vue\"\r\nimport nature_header from \"@/components/nature/Header.vue\"\r\nimport progress_dash1 from \"@/components/nature/progress/dash1/dash1.vue\"\r\nimport progress_dash2 from \"@/components/nature/progress/dash2/dash2.vue\"\r\nimport progress_dash3 from \"@/components/nature/progress/dash3/dash3.vue\"\r\nimport progress_dash4 from \"@/components/nature/progress/dash4/dash4.vue\"\r\nimport LoadingBar from \"@/components/Loading.vue\"\r\n\r\nimport { computed , ref} from 'vue';\r\nimport { useStore } from 'vuex'\r\nimport axios from 'axios'\r\nexport default {\r\n    name :\"progress\",\r\n    components:{\r\n        navigation,\r\n        nature_header,\r\n        progress_dash1,\r\n        progress_dash2,\r\n        progress_dash3,\r\n        progress_dash4,\r\n        LoadingBar\r\n    },\r\n    setup() {\r\n        var store = useStore()\r\n        var group_list = computed(() => store.state.group_list).value\r\n        var selected_company = ref(group_list[0])\r\n        store.commit(\"insight_select_company\",selected_company.value)\r\n        var rerender_signal = ref(0)\r\n\r\n        var now = new Date();\t// 현재 날짜 및 시간\r\n        var year = ref(now.getFullYear())\t// 년도\r\n        var lastyear = year.value - 1\r\n        var month = ref(now.getMonth()) //월\r\n\r\n        var server = ref(0)\r\n\r\n        const config = {\r\n            headers:{\r\n                Authorization:\"Bearer\"+\" \"+store.state.accessToken,\r\n            }\r\n        }\r\n\r\n        //기준량 데이터\r\n        async function get_Base_Info(){\r\n            var url = \"/CarbonNature/Evaluation/\"+selected_company.value\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getBaseYear',res.data.BaseYear)\r\n              store.commit('getBaseData',res.data.BaseEmissions)\r\n            })\r\n            .catch(error => {\r\n            console.log(error)\r\n             store.commit('getBaseYear',0)\r\n             store.commit('getBaseData',0)\r\n            })\r\n            .finally(()=>{\r\n                server.value = server.value + 1\r\n                console.log(server.value)\r\n            })\r\n\r\n            //감축 목표 총 데이터\r\n            var url = \"/CarbonNature/CarbonYear/\"+selected_company.value+\"/\"+year.value+\"/0\"\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getTargetData',res.data)\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n            })\r\n\r\n            //카테고리별 감축 목표 총 데이터\r\n            var url = \"/CarbonNature/CarbonYear/\"+selected_company.value+\"/\"+year.value+\"/1\"\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getTargetList',res.data)\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n            })\r\n\r\n            //현재 총 데이터\r\n            var url = \"/CarbonEmission/PartEmission/\"+selected_company.value+\"/\"+year.value+\"-01-01/\"+year.value+\"-\"+month.value+\"-28/0\"\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getTotalNowData',sumfun(res.data))\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n            })\r\n\r\n            //작년 총 데이터\r\n            var url = \"/CarbonEmission/PartEmission/\"+selected_company.value+\"/\"+lastyear+\"-01-01/\"+lastyear+\"-12-28/0\"\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getTotalLastData',sumfun(res.data))\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n            })\r\n\r\n            //작년 카테고리별 데이터\r\n            var url = \"/CarbonEmission/PartEmission/\"+selected_company.value+\"/\"+lastyear+\"-01-01/\"+lastyear+\"-12-28/1\"\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getTotalLastCategoryDataList',res.data)\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n            })\r\n\r\n            var url = \"/CarbonEmission/PartEmission/\"+selected_company.value+\"/\"+year.value+\"-01-01/\"+year.value+\"-\"+month.value+\"-28/1\"\r\n            console.log('카테고리별 현재 데이터', url)\r\n            await axios.get(url,config).then(res=>{\r\n                store.commit('getCategoryTotalList',res.data)\r\n                })\r\n                .catch(error => {\r\n                console.log(error)\r\n                })\r\n                .finally(()=>{\r\n                    server.value = server.value + 1\r\n                    console.log(server.value)\r\n                    rerender_signal =+1\r\n            })\r\n        }\r\n\r\n        function sumfun(list){\r\n            var sum =ref(0)\r\n            for(var i=0; i<list.length; i++){\r\n                sum.value = list[i] + sum.value\r\n            }\r\n            return sum.value\r\n        }\r\n\r\n        get_Base_Info()\r\n\r\n\r\n        \r\n        function change_company(){\r\n            store.commit(\"insight_select_company\",selected_company.value)\r\n            server.value = 0\r\n            get_Base_Info()\r\n        }\r\n        return{\r\n            group_list,\r\n            change_company,\r\n            selected_company,\r\n            rerender_signal,\r\n            server\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n#progress-dash1{\r\n    float:left;\r\n}\r\n\r\n#progress-dash2{\r\n    float:left;\r\n    margin-left:3.7vw;\r\n}\r\n#progress-dash3{\r\n    float:left;\r\n    margin-left:3.7vw;\r\n    margin-top:6vh\r\n}\r\n #progress-dash4{\r\n    clear:left;\r\n    float:left;\r\n    margin-top:6vh\r\n}\r\n</style>\r\n"],"mappings":";;EACSA,KAAK,EAAC;AAAc;;EAEhBA,KAAK,EAAC,UAAU;EAACC,KAAoB,EAApB;IAAA;EAAA;;;EAEbD,KAAK,EAAC;AAAY;gCAInBE,mBAAA,CAA6C;EAAxCF,KAAK,EAAC;AAAa,GAAC,gBAAc;;;;;EAE9BA,KAAK,EAAE;AAAM;;;;;EAUbA,KAAK,EAAC;AAAc;gCAErBE,mBAAA,CAAsD;EAAjDF,KAAK,EAAC;AAAc,GAAC,wBAAsB;;;;;;;;;;uBAtBpEG,mBAAA,CA2BM,OA3BNC,UA2BM,GA1BFC,YAAA,CAAgCC,qBAAA;IAApBN,KAAK,EAAC;EAAY,IAC9BE,mBAAA,CAwBM,OAxBNK,UAwBM,GAvBFF,YAAA,CAAgBG,wBAAA,GAChBN,mBAAA,CAqBM,OArBNO,UAqBM,G,gBApBFP,mBAAA,CAES;IAFDF,KAAK,EAAC,cAAc;+DAAUU,MAAA,CAAAC,gBAAgB,GAAAC,MAAA;IAAGC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAAAF,MAAA,IAAEF,MAAA,CAAAK,cAAc,CAACL,MAAA,CAAAC,gBAAgB;yBAC9FR,mBAAA,CAAkEa,SAAA,QAAAC,WAAA,CAA3CP,MAAA,CAAAQ,UAAU,EAAlBC,IAAI;yBAAnBhB,mBAAA,CAAkE;MAA9BiB,GAAG,EAAED;IAAI,GAAAE,gBAAA,CAAKF,IAAI;2FADlBT,MAAA,CAAAC,gBAAgB,E,GAGtDW,UAA6C,EAClCZ,MAAA,CAAAa,MAAM,S,cAAjBpB,mBAAA,CAQM,OAAAqB,UAAA,GAPFtB,mBAAA,CAEM,OAFNuB,UAEM,I,cADFC,YAAA,CAA4EC,yBAAA;IAA3DP,GAAG,EAAEV,MAAA,CAAAkB,eAAe;IAAEC,EAAE,EAAC;wBAG9CH,YAAA,CAA4EI,yBAAA;IAA3DV,GAAG,EAAEV,MAAA,CAAAkB,eAAe;IAAEC,EAAE,EAAC;sBAC1CH,YAAA,CAA4EK,yBAAA;IAA3DX,GAAG,EAAEV,MAAA,CAAAkB,eAAe;IAAEC,EAAE,EAAC;sBAC1CH,YAAA,CAA4EM,yBAAA;IAA3DZ,GAAG,EAAEV,MAAA,CAAAkB,eAAe;IAAEC,EAAE,EAAC;WAG9BnB,MAAA,CAAAa,MAAM,S,cAAtBpB,mBAAA,CAKM,OAAA8B,UAAA,GAJF/B,mBAAA,CAGM,OAHNgC,UAGM,GAFF7B,YAAA,CAAyB8B,qBAAA,GACzBC,UAAsD,C"},"metadata":{},"sourceType":"module","externalDependencies":[]}