e :\"dashboard2_evaluation\",\r\n      data() {\r\n        return {\r\n            input: false, // 모달창 현재 상태(false: 닫힘)\r\n        }\r\n      },\r\n      methods: {\r\n      },\r\n      components:{\r\n        popup_inputStandardVue\r\n      },\r\n      \r\n      async setup(props, { expose }) {\r\n        const store = useStore();\r\n        var group_name = computed(()=> store.state.insight_selected_company)\r\n        //서버\r\n        var server_evaluation={BaseYear:2019, BaseEmissions:2650}\r\n\r\n\r\n        const config = {\r\n            headers:{\r\n                Authorization:\"Bearer\"+\" \"+store.state.accessToken,\r\n                \"Content-Type\": \"text/html; charset=euc-kr\",\r\n            }\r\n        }\r\n            \r\n        async function base_emissions(){\r\n            console.log(store.state.accessToken)\r\n            var url = \"/CarbonNature/Evaluation/공과대학\"\r\n            console.log(url)\r\n            axios.get(\"/CarbonNature/Evaluation/공과대학\",config).then(res=>{\r\n                server_evaluation.value = res.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n            .finally(()=>{\r\n\r\n            })\r\n        }\r\n        await base_emissions()\r\n \r\n        var baseYear = server_evaluation.BaseYear\r\n        var baseEmissions = server_evaluation.BaseEmissions\r\n        var 기관명 = '기업'\r\n        var  위치 =  '경남 진주시'\r\n        var  scope1 = 123.4\r\n        function open_popup(){\r\n            console.log('popup')\r\n            store.commit('OnGroupInfo')\r\n        }\r\n\r\n        return{\r\n            group_name,기관명,위치,scope1, open_popup,baseYear,baseEmissions,base_emissions\r\n        }\r\n    }\r\n  }\r\n</script>\r\n  "],"mappings":"AA0CA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAACC,GAAE,QAAS,KAAK;AAClC,OAAOC,sBAAqB,MAAO,2BAA2B;AAC9D,OAAOC,KAAI,MAAO,OAAO;AACvB,eAAe;EACXC,IAAG,EAAG,uBAAuB;EAC7BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACHC,KAAK,EAAE,KAAK,CAA